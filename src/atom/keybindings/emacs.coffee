window.keymap.bindKeys '.editor',
  'ctrl-f': 'move-right'
  'ctrl-b': 'move-left'
  'ctrl-p': 'move-up'
  'ctrl-n': 'move-down'
  'alt-f': 'move-to-end-of-word'
  'alt-b': 'move-to-beginning-of-word'
  'ctrl-a': 'move-to-first-character-of-line'
  'ctrl-e': 'move-to-end-of-line'
  'ctrl-h': 'backspace'
  'ctrl-d': 'delete'
